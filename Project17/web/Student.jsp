
public class Student {
    private int sid;
    private String snm;
    private int age;

    public void setSid(int sid) {
        this.sid = sid;
    }

    public void setSnm(String snm) {
        this.snm = snm;
    }

    public void setAge(int age) {
        this.age = age;
    }

    public int getSid() {
        return sid;
    }

    public String getSnm() {
        return snm;
    }

    public int getAge() {
        return age;
    }
    
}
